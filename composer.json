{
    "require": {
        "ext-yaml": "*",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "bench": "phpbench run tests/Benchmark --report=default",
        "cs": "php-cs-fixer fix --verbose"
    }
}
